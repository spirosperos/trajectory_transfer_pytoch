<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <!-- Object dimensions -->
  <xacro:property name="length" value="0.057"/>
  <xacro:property name="width"  value="0.185"/>
  <xacro:property name="height" value="0.187"/>

  <!-- Mass -->
  <xacro:property name="mass" value="2.6"/>

  <!-- Move mesh to origin -->
  <xacro:macro name="set_origin">
    <origin xyz="-0.023 0.045 0.082" rpy="${91.5*pi/180} ${1.5*pi/180} ${88*pi/180}"/>
  </xacro:macro>
</robot>
